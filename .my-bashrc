#
# ~/.bashrc
#
[[ -f ~/.local/bin/git-prompt.sh ]] && . ~/.local/bin/git-prompt.sh
GIT_PS1='$(__git_ps1 "(%s)")$ '
PS1="$PS1$GIT_PS1"
HISTSIZE=-1
HISTFILESIZE=-1
PATH=$PATH:~/.local/bin
if [[ -d ~/git/devops/devtools/bin/ ]]; then
    PATH=$PATH:~/git/devops/devtools/bin
fi
TERMINAL=`which terminator`
EDITOR=vim
alias tsm="transmission-remote"
